mixin mnu()
	-
		var nav= { 
			"Формула успеха":"#",
			"Инструменты":"#",
			"Стратегии ":"#",
			"Автор":"#" ,
			"Программа":"#",
			}
	+e.UL.nav(itemscope itemtype="http://schema.org/SiteNavigationElement")
		each linkPatch, linkName in nav
			+e.LI.item( itemprop="item" ): +e.A.link(href= linkPatch  itemprop="url")= linkName
//- /menu